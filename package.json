{
  "name": "tap-arc",
  "description": "spec-like TAP reporter",
  "author": "tbeseda",
  "version": "0.3.4",
  "license": "Apache-2.0",
  "main": "index.js",
  "bin": {
    "tap-arc": "index.js"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architect/tap-arc.git"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "test",
    "tap",
    "spec",
    "tap-spec",
    "tape",
    "reporter"
  ],
  "scripts": {
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "make-snapshots": "node scripts/make-snapshots.js",
    "node:passing": "node test/fixtures/node/create-passing-tap.mjs",
    "node:simple": "node test/fixtures/node/create-simple-tap.mjs",
    "slow": "npm run --silent tape:slow-pass | ./index.js",
    "tap-arc:node-passing": "npm run --silent node:passing | ./index.js",
    "tap-arc:node-simple": "npm run --silent node:simple | ./index.js",
    "tap-arc:node-simple:-v": "npm run --silent node:simple | ./index.js -v",
    "tap-arc:tape-diff:-v": "npm run --silent tape:diff | ./index.js -v",
    "tap-arc:tape-diff": "npm run --silent tape:diff | ./index.js",
    "tap-arc:tape-empty": "npm run --silent tape:empty | ./index.js",
    "tap-arc:tape-mixed": "npm run --silent tape:mixed | ./index.js",
    "tap-arc:tape-upstream-error": "npm run --silent tape:upstream-error | ./index.js",
    "tap-arc:tape-passing": "npm run --silent tape:passing | ./index.js",
    "tap-arc:tape-simple:-p": "npm run --silent tape:simple | ./index.js -p",
    "tap-arc:tape-simple:-v": "npm run --silent tape:simple | ./index.js -v",
    "tap-arc:tape-simple": "npm run --silent tape:simple | ./index.js",
    "tape:diff": "tape test/fixtures/tape/create-diff-tap.js",
    "tape:empty": "tape test/fixtures/tape/create-empty-tap.js",
    "tape:mixed": "tape test/fixtures/tape/create-mixed-tap.js",
    "tape:upstream-error": "tape test/fixtures/tape/create-upstream-error-tap.js",
    "tape:passing": "tape test/fixtures/tape/create-passing-tap.js",
    "tape:simple": "tape test/fixtures/tape/create-simple-tap.js",
    "tape:slow-pass": "tape test/fixtures/tape/create-slow-passing-tap.js",
    "node": "node test/node.test.js | ./index.js",
    "tape": "tape test/tape.test.js | ./index.js",
    "test": "npm run lint:check && npm run tape && npm run node"
  },
  "dependencies": {
    "json5": "^2.2.1",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "strip-ansi": "^6.0.1",
    "tap-parser": "^11.0.1",
    "tcompare": "^5.0.7",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@architect/eslint-config": "^2.0.1",
    "@types/node": "^17.0.25",
    "eslint": "^8.13.0",
    "tape": "5.5.3"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config"
  }
}
